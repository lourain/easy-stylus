{
  "name": "hello-world",
  "displayName": "hello world",
  "description": "hello world",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.36.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:stylus",
    "onCommand:compile.stylus"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": {
      "command": "compile.stylus",
      "title": "compile stylus to css"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.36.0",
    "glob": "^7.1.4",
    "mocha": "^10.2.0",
    "typescript": "^3.3.1",
    "tslint": "^5.12.1",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "@types/stylus": "^0.48.31",
    "stylus": "^0.54.5"
  }
}
